{
	"compilerOptions": {
		"target": "ESNext",
		"jsx": "preserve",
		"moduleResolution": "node",
		"jsxImportSource": "@builder.io/mitosis",
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"outDir": "./dist" // Add this line
	},
	"include": ["src"]
}
